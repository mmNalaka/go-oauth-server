datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model User {
    id            String  @id @default(uuid())
    email         String  @unique
    emailVerified Boolean @default(false)
    givenName     String
    familyName    String?
    picture       String?
}

model Roles {
    id Int @id @default(autoincrement())
}

model Permission {
    id Int @id @default(autoincrement())
}

model Provider {
    id   Int    @id @default(autoincrement())
    name String
}

model AccessToken {
    id   Int    @id @default(autoincrement())
    name String
}
